{
    "env": {
        "development": {
            "presets": [
                "env",
                "react",
                "flow"
            ],
            "plugins": [
                "transform-react-jsx-source",
                "react-hot-loader/babel"
            ]
        },
        "production": {
            "presets": [
                [
                  "env",
                  {
                    "es2015": {
                      "modules": false
                    },
                    "targets": {
                      "ie": 9,
                      "uglify": true
                    }
                  }
                ],
                "react",
                "flow"
              ],
            "plugins": [
                "transform-react-remove-prop-types"
            ]
        }
    }
}
